{
	"name": "ubumaths",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"test": "playwright test",
		"vitest": "vitest",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark && npm run site-light && npm run site-dark",
		"site-light": "sass --no-source-map -I src/theme -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site.css",
		"site-dark": "sass --no-source-map -I src/theme/dark -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site-dark.css",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@mdi/js": "^6.7.96",
		"@playwright/test": "^1.21.0",
		"@smui-extra/accordion": "^6.0.0-beta.16",
		"@smui-extra/badge": "^6.0.0-beta.16",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/common": "^6.0.0-beta.16",
		"@smui/drawer": "^6.0.0-beta.16",
		"@smui/fab": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@smui/paper": "^6.0.0-beta.16",
		"@smui/select": "^6.0.0-beta.16",
		"@smui/slider": "^6.0.0-beta.16",
		"@smui/tab": "^6.0.0-beta.16",
		"@smui/tab-bar": "^6.0.0-beta.16",
		"@smui/tooltip": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-vercel": "^1.0.0-next.56",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.3",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^19.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.24",
		"vitest": "^0.15.0",
		"vitest-svelte-kit": "^0.0.6"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^1.35.3",
		"svelte-viewport-info": "^1.0.0",
		"tinycas": "^0.8.8",
		"tinymathlive": "^0.73.8"
	}
}
